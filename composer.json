{
  "name": "netflex/structures",
  "description": "Netflex Structure library",
  "type": "library",
  "license": "MIT",
  "keywords": [],
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Netflex\\Structure\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/apility/seotools"
    }
  ],
  "replace": {
    "netflex/structure": "*"
  },
  "require": {
    "php": "^7.3",
    "netflex/pages": "^3.0",
    "netflex/query-builder": "^3.0",
    "netflex/support": "^3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "illuminate/cache": "^8.0",
    "illuminate/container": "^8.0",
    "illuminate/events": "^8.0",
    "illuminate/filesystem": "^8.0",
    "illuminate/support": "^8.0",
    "larapack/dd": "^1.1",
    "vlucas/phpdotenv": "^3.6"
  }
}
